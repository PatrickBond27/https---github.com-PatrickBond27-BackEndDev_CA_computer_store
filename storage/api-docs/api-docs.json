                                        "example": "512 GB"
                                    },
                                    "ram": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "8 GB"
                                    },
                                    "price": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "1600"
                                    },
                                    "brand_id": {
                                        "type": "json",
                                        "format": "json",
                                        "example": 2
                                    },
                                    "developers": {
                                        "type": "json",
                                        "format": "json",
                                        "example": [
                                            1,
                                            3
